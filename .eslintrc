{
	"extends": [
		"plugin:flowtype/recommended",
		"prettier"
	],
	"parser": "babel-eslint",
	"plugins": [
		"flowtype"
	],
	"rules": {
		"import/prefer-default-export": "off",
		"import/extensions": "off",
		"import/no-extraneous-dependencies": ["off"]
	},
	"settings": {
		"import/resolver": {
			"babel-module": {}
		}
	}
}
